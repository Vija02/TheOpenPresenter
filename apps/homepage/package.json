{
  "name": "@repo/homepage",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "cd src && NODE_ENV=production NODE_OPTIONS=\"${NODE_OPTIONS:-} -r @repo/config/env\" next build",
    "codegen": "cd src && npx @tailwindcss/cli -i ./src/app/css/style.css -o ./src/app/css/compiled.css",
